node {
    stage('SCM') {
        echo "Gathering code from version control..."
        git branch: '${branch}', url: 'https://github.com/gtvfx/jenkins_local'
    }
    stage('Build') {
        echo 'Building...'
        bat 'dotnet --version'
        bat 'dotnet build ConsoleApp1'
        echo 'Building new feature'
    }
    stage('test') {
        echo 'Testing...'
    }
    stage('deploy') {
        echo 'Deploying...'
    }
}